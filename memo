drop sequence memseq;

create sequence y_memseq  
increment by 1
start with 1
minvalue 1
maxvalue 10000
nocache;

drop table y_members purge ;

create table y_members(
no number primary key,
name varchar2(50) not null,
id varchar2(20) not null,
password varchar2(20),
email varchar2(30),
phone varchar2(30)
);


col name for  a12
col id for a12
col password for a12


create sequence foodseq 
increment by 1
start with 1
minvalue 1
maxvalue 10000
nocache;

drop table food;
create table food(
pnum number primary key,
pname varchar2(40) not null,
pkind_fk varchar2(20) not null,
pqty number default 0,
pimage varchar2(50),
price number default 0,
pcontent varchar2(100),
pspec varchar2(30),
point number default 0,
totalPrice number,
salePrice number,
totalpoint number
);


create sequence f_kindseq 
increment by 1
start with 1
minvalue 1
maxvalue 10000
nocache;

drop table f_kind;
create table f_kind(
fnum number primary key,
fcode varchar2(20) not null,
fname varchar2(20) not null);


drop table y_board;
create table y_board(
	num number not null primary key,
	writer varchar2(10) not null,
	subject varchar2(20) not null,
	passwd varchar2(12) not null,
	reg_date date not null,
	readcount int default 0,
	ref number  default 0 not null,
	re_step number default 0 not null,
	re_level number default 0 not null,
	content varchar2(50) not null,
);
drop sequence board_seq;
create sequence board_seq
increment by 1
start with 1
minvalue 1
maxvalue 10000
nocache;

drop sequence board_seq ;
create sequence board_seq nocache;  


commit ;
